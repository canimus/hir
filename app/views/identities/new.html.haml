%h1 HIR Casa 2.0
= form_tag "/auth/identity/register" do
  = text_field_tag :name, nil, placeholder: "Name"
  = email_field_tag :email, nil, placeholder: "Email"
  = password_field_tag :password, nil, placeholder: "Password"
  = password_field_tag :password_confirmation, nil, placeholder: "Confirmation"
  = submit_tag
